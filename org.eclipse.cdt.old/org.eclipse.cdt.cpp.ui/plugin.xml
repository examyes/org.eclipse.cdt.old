<?xml version="1.0"?>
<plugin
   name = "%pluginName"
   id = "com.ibm.cpp.ui"
   version = "1.0.0"
   vendor-name = "International Business Machines Corp."
   class="com.ibm.cpp.ui.internal.CppPlugin">

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.boot"/>
      <import plugin="com.ibm.lpex"/>
      <import plugin="com.ibm.debug"/>
      <import plugin="org.eclipse.search"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.core.target"/>
      <import plugin="org.eclipse.vcm.core"/>
      <import plugin="com.ibm.dstore.miners"/>
      <import plugin="com.ibm.cpp.miners"/>
      <import plugin="com.ibm.cpp.miners.parser"/>
      <import plugin="com.ibm.dstore.extra"/>
      <import plugin="com.ibm.dstore.core"/>
      <import plugin="com.ibm.dstore.ui"/>
      <import plugin="com.ibm.dstore.hosts"/>
      <import plugin="com.ibm.cpp.linux.help"/>
   </requires>

   <runtime>
     <library name=".">
       	<export name="*" />
     </library>
     <library name="cpp_ui.jar">
       	<export name="*" />
     </library>
     
   </runtime>


<!-- C++ Project Nature -->

<extension 
	point="org.eclipse.core.resources.natures" 
	id="cppnature"
	name="C/C++ Project Nature">
	<runtime>
		<run class="com.ibm.cpp.ui.internal.CppProject">
		</run>
	</runtime>
</extension>



<!-- Launchers -->

<extension point="org.eclipse.debug.core.launchers">
	<launcher  id="com.ibm.cpp.ui.launchers.CppLoadLauncher"
	   	   class="com.ibm.cpp.ui.internal.launchers.CppLoadLauncher"
	  	   modes="debug"
 	  	   label= "Start a C/C++ Application"
 	  	   public= "true"
 	  	   description= "C/C++ debug program launcher" />

	<launcher  id="com.ibm.cpp.ui.launchers.CppRunLauncher"
	   	   class="com.ibm.cpp.ui.internal.launchers.CppRunLauncher"
	  	   modes="run"
 	  	   label= "C/C++ Application"
 	  	   public= "true"
 	  	   description= "C/C++ run program launcher" />

	<launcher  id="com.ibm.cpp.ui.launchers.CppAttachLauncher"
	   	   class="com.ibm.cpp.ui.internal.launchers.CppAttachLauncher"
	  	   modes="debug"
 	  	   label= "Attach to C/C++ Application"
 	  	   public= "true"
 	  	   description= "C/C++ attach to program launcher" />
</extension>

<extension
	point="org.eclipse.ui.perspectives">
   <perspective
	id="com.ibm.cpp.ui.CppPerspective"
	name="%CppPerspective"
	class="com.ibm.cpp.ui.internal.views.CppPerspective">
   </perspective>
<!--
   <perspective
	id="com.ibm.cpp.ui.MinersPerspective"
	name="%Miner_Development"
	class="com.ibm.cpp.ui.internal.views.MinersPerspective">
   </perspective>
-->
</extension>

<extension point="org.eclipse.ui.perspectiveExtensions">
	<perspectiveExtension 
		targetID="org.eclipse.ui.resourcePerspective">
		<perspectiveShortcut id="com.ibm.cpp.ui.CppPerspective"/>
	</perspectiveExtension>
	<perspectiveExtension 
		targetID="org.eclipse.vcm.ui.vcmPerspective">
		<perspectiveShortcut id="com.ibm.cpp.ui.CppPerspective"/>
	</perspectiveExtension>
	<perspectiveExtension 
		targetID="com.ibm.cpp.ui.CppPerspective">
		<perspectiveShortcut id="com.ibm.cpp.ui.CppPerspective"/>
	</perspectiveExtension>
	<perspectiveExtension 
		targetID="com.ibm.cpp.ui.CppPerspective">
		<perspectiveShortcut id="org.eclipse.debug.ui.DebugPerspective"/>
	</perspectiveExtension>
	<perspectiveExtension 
		targetID="org.eclipse.ui.resourcePerspective">
		<perspectiveShortcut id="com.ibm.cpp.ui.CppPerspective"/>
	</perspectiveExtension>
	<perspectiveExtension 
		targetID="org.eclipse.debug.ui.DebugPerspective">
		<perspectiveShortcut id="com.ibm.cpp.ui.CppPerspective"/>
	</perspectiveExtension>
</extension>

<extension
