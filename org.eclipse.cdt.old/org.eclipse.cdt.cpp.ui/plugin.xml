	</view>
-->
	<view
		id="com.ibm.cpp.ui.Targets"
		name="%Run_Specifications"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/objects.gif"
		class="com.ibm.cpp.ui.internal.views.targets.Targets">
	</view>
        <view
		id="com.ibm.cpp.ui.CppProjectsViewPart"
		name="%C++_Projects"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/objects.gif"
		class="com.ibm.cpp.ui.internal.views.CppProjectsViewPart">
	</view>
</extension>


  <extension point = "org.eclipse.ui.popupMenus">
        <objectContribution
           id="com.ibm.cpp.ui.internal.menu"
           objectClass="org.eclipse.core.resources.IFile">
	   <menu
		id="makefileMenu"
		path="additions"
		label="%Makefile___">
		<separator name = "makefileMenu"/>
	   </menu>
	   <action
              id="designate"
	      label="%Designate"
	      menubarPath="makefileMenu/designateGroup"
	      class="com.ibm.cpp.ui.internal.actions.CppActionDelegateDesignate"	
	      enablesFor="1">
	   </action>
	   <action
              id="com.ibm.cpp.ui.internal.action1"
	      label="%Undesignate"
	      menubarPath="makefileMenu/designateGroup"
	      class="com.ibm.cpp.ui.internal.actions.CppActionDelegateUnDesignate"	
	      enablesFor="1">
	   </action>
	   <action
              id="com.ibm.cpp.ui.internal.action2"
	      label="%Build"
	      menubarPath="makefileMenu/designateGroup"
	      class="com.ibm.cpp.ui.internal.actions.CppActionDelegateBuild"	
	      enablesFor="1">
	   </action>
	</objectContribution>

        <objectContribution
           id="com.ibm.cpp.ui.internal.menu4"
           objectClass="org.eclipse.core.resources.IFolder">
	   <menu
		id="folderCommandMenu"
		path="additions"
		label="%Issue_Command___">
		<separator name = "folderCommandMenu"/>
	   </menu>
	   <action
              id="com.ibm.cpp.ui.internal.actionFolderBuild"
	      label="%Command"
	      menubarPath="folderCommandMenu/folderCommandGroup"
	      class="com.ibm.cpp.ui.internal.actions.CppActionDelegateFolderBuild"	
	      enablesFor="1">
	   </action>
	</objectContribution>
        <objectContribution
           id="com.ibm.cpp.ui.internal.menu5"
           objectClass="org.eclipse.core.resources.IFile">
	   <menu
		id="fileParseMenu"
		path="additions"
		label="%Parse">
		<separator name = "fileParseMenu"/>
	   </menu>
	   <action
              id="com.ibm.cpp.ui.internal.actionParse0"
	      label="%File"
	      menubarPath="fileParseMenu/ParseGroup"
              class="com.ibm.cpp.ui.internal.actions.ParseAction"
	      enablesFor="1">
	   </action>
	</objectContribution>
        <objectContribution
           id="com.ibm.cpp.ui.internal.menu6"
           objectClass="org.eclipse.core.resources.IFolder">
	   <menu
		id="folderParseMenu"
		path="additions"
		label="%Parse___">
		<separator name = "folderParseMenu"/>
	   </menu>
	   <action
              id="com.ibm.cpp.ui.internal.actionParse1"
	      label="%Folder"
	      menubarPath="folderParseMenu/ParseGroup"
              class="com.ibm.cpp.ui.internal.actions.ParseAction"
	      enablesFor="1">
	   </action>
	</objectContribution>
        <objectContribution
           id="com.ibm.cpp.ui.internal.menu7"
           objectClass="org.eclipse.core.resources.IProject">
	   <menu
		id="parseMenu"
		path="additions"
		label="%Parse___">
		<separator name = "parseMenu"/>
	   </menu>
	   <action
              id="com.ibm.cpp.ui.internal.actionClean"
	      label="%Clean_Parse"
	      menubarPath="parseMenu/ParseGroup"
              class="com.ibm.cpp.ui.internal.actions.ClearAction"
	      enablesFor="1">
	   </action>
	   <action
              id="com.ibm.cpp.ui.internal.actionSave"
	      label="%Save_Parse"
	      menubarPath="parseMenu/ParseGroup"
              class="com.ibm.cpp.ui.internal.actions.SaveAction"
	      enablesFor="1">
	   </action>
	   <action
              id="com.ibm.cpp.ui.internal.actionParse2"
	      label="%Project"
	      menubarPath="parseMenu/ParseGroup"
	      class="com.ibm.cpp.ui.internal.actions.ParseAction"
	      enablesFor="1">
	   </action>
	</objectContribution>
        <objectContribution
           id="com.ibm.cpp.ui.internal.menu9"
           objectClass="org.eclipse.core.resources.IProject">
	   <action
              id="com.ibm.cpp.ui.internal.views.targets.CreateTargetAction"
	      label="Create Specification"
	      path="additions"
	      class="com.ibm.cpp.ui.internal.views.targets.CreateTargetAction"	
	      enablesFor="1">
	   </action>
	   <action
              id="convert"
	      label="Support C/C++"
	      path="additions"
	      class="com.ibm.cpp.ui.internal.actions.ConvertToCppProject"	
	      enablesFor="1">
	   </action>

	</objectContribution>
  </extension>

  <extension point = "org.eclipse.ui.editors">
	<editor
        	id = "com.ibm.cpp.ui.internal.editor.CppEditor"
      		name="%Lpex_Editor"
		icon="icons/lpex.gif"
      		extensions="app,asm,c,cc,cpp,cpy,cxx,h,hh,hla,hpp,inc,inl,am,in,dat"
      		class="com.ibm.cpp.ui.internal.editor.CppEditor"
		contributorClass="com.ibm.cpp.ui.internal.editor.contributor.CppEditorContextContributor">
	</editor>
   </extension>

<extension point="org.eclipse.core.resources.builders"
      	id="cppbuilder"
	name="%C++_Builder">
	<builder>
		<run class="com.ibm.cpp.ui.internal.builder.CppBuilder">
		</run>
	</builder>
</extension>


<extension
	point="org.eclipse.ui.newWizards">
	<category
	   id="com.ibm.cpp.ui.cpp"
	   name="%C++">
	</category>
	<wizard
	   id="com.ibm.cpp.ui.wizards.CppNewProjectResourceWizard"
	   name="%C++_Project"
	   availableAsShortcut="true"
	   class="com.ibm.cpp.ui.internal.wizards.CppNewProjectResourceWizard"
	   category="com.ibm.cpp.ui.cpp"
	   project="true"
	   finalPerspective="com.ibm.cpp.ui.CppPerspective"
	   icon="icons/newprj_wiz.gif">
           <description>%Create_a_C++_Project</description>
        </wizard>
</extension>


<extension
	point="org.eclipse.ui.propertyPages">
	<page
		id="com.ibm.cpp.ui.internal.builder.BuildPropertyPage"
		name="%Build_Invocation"
		objectClass="org.eclipse.core.resources.IProject"
		class="com.ibm.cpp.ui.internal.builder.BuildPropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
	<page
		id="com.ibm.cpp.ui.internal.builder.ParsePathPropertyPage"
		name="%Parse_Include_Path"
		objectClass="org.eclipse.core.resources.IProject"
		class="com.ibm.cpp.ui.internal.builder.ParsePathPropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
	<page
		id="com.ibm.cpp.ui.internal.builder.ParseQualityPropertyPage"
		name="%Parse_Quality"
		objectClass="org.eclipse.core.resources.IProject"
		class="com.ibm.cpp.ui.internal.builder.ParseQualityPropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
	<page
		id="com.ibm.cpp.ui.internal.builder.AutoParsePropertyPage"
		name="%Parse_Behaviour"
		objectClass="org.eclipse.core.resources.IProject"
		class="com.ibm.cpp.ui.internal.builder.AutoParsePropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
	<page
		id="com.ibm.cpp.ui.internal.builder.EnvironmentPropertyPage"
		name="%Environment"
		objectClass="org.eclipse.core.resources.IProject"
		class="com.ibm.cpp.ui.internal.builder.EnvironmentPropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
	<page
		id="com.ibm.cpp.ui.internal.builder.RemotePropertyPage"
		name="Remote Properties"
		objectClass="com.ibm.cpp.ui.internal.vcm.Repository"
		class="com.ibm.cpp.ui.internal.builder.RemotePropertyPage">
	       <filter name="nature" value="com.ibm.cpp.ui.cppnature"/>
        </page>
</extension>

<extension point="org.eclipse.search.searchPages">
   <page  id="com.ibm.cpp.ui.CppSearchPage"
         icon="icons/search.gif"
         label="%C++_Search"
		 sizeHint="400,240"
		 extensions="cpp:90, C:90, c:90, hpp:90, H:90, h:90, *:1"
		 class="com.ibm.cpp.ui.internal.views.search.CppSearchPage">
   </page>
</extension>
 
<extension id="cppsearchmarker" point="org.eclipse.core.resources.markers">
	<super type="org.eclipse.core.resources.searchmarker"/>
	<attribute name="DataElementID"/>
</extension>

<extension
	point="org.eclipse.ui.preferencePages">
	<page name="%C++"
		id="com.ibm.cpp.ui.internal.preferences.CppBasePreferencePage"
		class="com.ibm.cpp.ui.internal.preferences.CppBasePreferencePage">
	</page>
	<page name="%Default_Environment"
		id="com.ibm.cpp.ui.internal.preferences.EnvironmentPreferencePage"
