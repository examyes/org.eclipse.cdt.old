		class="com.ibm.cpp.ui.internal.views.DetailsViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.SelectedObjectViewPart"
		name="Selected Object"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/details.gif"
		class="com.ibm.cpp.ui.internal.editor.contentoutliner.SelectedObjectViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.SuperDetailsViewPart"
		name="%More_Details"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/details.gif"
		class="com.ibm.cpp.ui.internal.views.SuperDetailsViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.CppCommandViewPart"
		name="%Commands"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/commands.gif"
		class="com.ibm.cpp.ui.internal.views.CppCommandViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.CppOutputViewPart"
		name="%C/C++ Output"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/output.gif"
		class="com.ibm.cpp.ui.internal.views.CppOutputViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.ProjectObjectsViewPart"
		name="%Project_Objects"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/objects.gif"
		class="com.ibm.cpp.ui.internal.views.ProjectObjectsViewPart">
	</view>
        <view
		id="com.ibm.cpp.ui.SystemObjectsViewPart"
		name="%System_Objects"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/objects.gif"
		class="com.ibm.cpp.ui.internal.views.SystemObjectsViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.ParsedSourceViewPart"
		name="%Parsed_Files"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/Parsed Source.gif"
		class="com.ibm.cpp.ui.internal.views.ParsedSourceViewPart">
	</view>
<!--
	<view
		id="com.ibm.cpp.ui.SchemaViewPart"
		name="%Schema"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/schema.gif"
		class="com.ibm.cpp.ui.internal.views.SchemaViewPart">
	</view>
	<view
		id="com.ibm.cpp.ui.MinersViewPart"
		name="%Miners"
		category="com.ibm.cpp.ui.cpp"
		icon="icons/miners.gif"
