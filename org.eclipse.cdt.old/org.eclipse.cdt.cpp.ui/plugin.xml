	</page>	
	<page name="%Output_View"
		id="com.ibm.cpp.ui.internal.preferences.OutputViewPreferencePage"
		class="com.ibm.cpp.ui.internal.preferences.OutputViewPreferencePage"
		category= "com.ibm.cpp.ui.internal.preferences.ViewPreferencePage">
	</page>	
</extension>

<extension
	point="org.eclipse.ui.actionSets">
	<actionSet 
		id="com.ibm.cpp.ui.CppActionSet"
		label="C/C++"
		visible="true"
		description="C/C++ Action Set">
		<action id="com.ibm.cpp.ui.ParseAction"
			toolbarPath="Normal/C++"			
			label="Parse"
			tooltip="Parse"
			icon="icons/parse.gif"
			class="com.ibm.cpp.ui.internal.actions.ParseAction"/>
		<action id="com.ibm.cpp.ui.CancelAction"
			toolbarPath="Normal/C++"			
			label="Cancel"
			tooltip="Cancel Running Jobs"
			icon="icons/cancel.gif"
			class="com.ibm.cpp.ui.internal.actions.CancelAction"/>
	</actionSet>

</extension>
	


</plugin>




