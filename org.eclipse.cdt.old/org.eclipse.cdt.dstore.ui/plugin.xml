<?xml version="1.0"?>
<plugin
   name = "DataStore UI Plugin"
   id = "org.eclipse.cdt.dstore.ui"
   version = "0.5.0"
   vendor-name = "International Business Machines Corp."
   class="org.eclipse.cdt.dstore.ui.DataStoreUIPlugin">

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.boot"/>
      <import plugin="org.eclipse.cdt.dstore.extra"/>
      <import plugin="org.eclipse.cdt.dstore.core"/>
   </requires>

   <runtime>
    <library name=".">
       	<export name="*" />      
     </library> 
    <library name="dstore_ui.jar">
       	<export name="*" />      
     </library> 
   </runtime>


<extension
	point="org.eclipse.ui.views">
	<category
	   id="org.eclipse.cdt.dstore.ui"
	   name="&amp;DataStore">
	</category>
	<view
		id="org.eclipse.cdt.dstore.ui.views.GenericViewPart"
		name="%Generic_View_Part"
		category="org.eclipse.cdt.dstore.ui"
		icon="icons/objects.gif"
		class="org.eclipse.cdt.dstore.ui.views.GenericViewPart">
	</view>
	<view
		id="org.eclipse.cdt.dstore.ui.views.DetailsViewPart"
		name="%Object_Details"
		category="org.eclipse.cdt.dstore.ui"
		icon="icons/details.gif"
		class="org.eclipse.cdt.dstore.ui.views.DetailsViewPart">
	</view>
</extension>

</plugin>







