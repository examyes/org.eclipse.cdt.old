<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.cdt.managedbuilder.core.buildDefinitions">
      <managedBuildRevision fileVersion="4.0.0"/>
      <toolChain
            archList="all"
            configurationEnvironmentSupplier="org.eclipse.cdt.windows.build.WindowsEnvironmentVariableSupplier"
            id="org.eclipse.cdt.windows.toolchain.base"
            name="%toolchain.name"
            osList="win32"
            targetTool="org.eclipse.cdt.windows.link;org.eclipse.cdt.windows.lib">
         <targetPlatform
               archList="all"
               binaryParser="org.eclipse.cdt.core.PE"
               id="org.eclipse.cdt.windows.targetPlatform"
               osList="win32">
         </targetPlatform>
         <builder
               id="org.eclipse.cdt.windows.builder"
               isVariableCaseSensitive="false"
               superClass="org.eclipse.cdt.build.core.internal.builder">
         </builder>
      <tool
            command="cl /c"
            errorParsers="org.eclipse.cdt.core.VCErrorParser"
            id="org.eclipse.cdt.windows.cl"
            name="%compiler.name"
            natureFilter="both"
            outputFlag="/Fo">
         <supportedProperties>
            <property
                  id="org.eclipse.cdt.build.core.buildType">
               <value
                     id="org.eclipse.cdt.build.core.buildType.debug">
               </value>
               <value
                     id="org.eclipse.cdt.build.core.buildType.release">
               </value>
            </property>
         </supportedProperties>
         <envVarBuildPath
               pathDelimiter=";"
               pathType="buildpathInclude"
               variableList="INCLUDE"/>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.optimization"
               name="Optimization"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.optimization"
               id="org.eclipse.cdt.windows.cl.option.optimization"
               name="Optimization"
               resourceFilter="all"
               valueType="enumerated">
            <enumeratedOptionValue
                  id="org.eclipse.cdt.windows.cl.option.optimization.default"
                  isDefault="false"
                  name="Default"/>
            <enumeratedOptionValue
                  command="/O1"
                  id="org.eclipse.cdt.windows.cl.option.optimization.1"
                  isDefault="false"
                  name="Minimize Size (/O1)"/>
            <enumeratedOptionValue
                  command="/O2"
                  id="org.eclipse.cdt.windows.cl.option.optimization.2"
                  isDefault="true"
                  name="Maximize Speed (/O2)"/>
            <enablement
                  attribute="value"
                  extensionAdjustment="false"
                  type="CONTAINER_ATTRIBUTE"
                  value="org.eclipse.cdt.windows.cl.option.optimization.2">
               <checkBuildProperty
                     property="org.eclipse.cdt.build.core.buildType"
                     value="org.eclipse.cdt.build.core.buildType.release">
               </checkBuildProperty>
            </enablement>
            <enablement
                  attribute="value"
                  extensionAdjustment="false"
                  type="CONTAINER_ATTRIBUTE"
                  value="org.eclipse.cdt.windows.cl.option.optimization.default">
               <checkBuildProperty
                     property="org.eclipse.cdt.build.core.buildType"
                     value="org.eclipse.cdt.build.core.buildType.debug">
               </checkBuildProperty>
            </enablement>
         </option>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               name="Code Generation"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               id="org.eclipse.cdt.windows.cl.option.arch"
               name="Minimum CPU Architecture"
               resourceFilter="all"
               valueType="enumerated">
            <enumeratedOptionValue
                  id="org.eclipse.cdt.windows.cl.option.arch.default"
                  isDefault="true"
                  name="Default"/>
            <enumeratedOptionValue
                  command="/arch:SSE"
                  id="org.eclipse.cdt.windows.cl.option.arch.sse"
                  isDefault="false"
                  name="SSE (/arch:SSE)"/>
            <enumeratedOptionValue
                  id="org.eclipse.cdt.windows.cl.option.arch.sse2"
                  isDefault="false"
                  name="SSE2 (/arch:SSE2)"/>
         </option>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               command="/clr"
               defaultValue="false"
               id="org.eclipse.cdt.windows.cl.option.clr"
               name="Common Language Runtime (/clr)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               command="/EHs"
               defaultValue="true"
               id="org.eclipse.cdt.windows.cl.option.exceptions"
               name="Enable C++ Exception Handling (/EHs)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               commandFalse="/GR-"
               defaultValue="true"
               id="org.eclipse.cdt.windows.cl.option.rtti"
               name="Enable Run-Time Type Information (/GR)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.codegen"
               id="org.eclipse.cdt.windows.cl.option.runtime"
               name="Run-Time Library"
               resourceFilter="all"
               valueType="enumerated">
            <enumeratedOptionValue
                  command="/MT"
                  id="org.eclipse.cdt.windows.cl.option.runtime.mt"
                  isDefault="false"
                  name="Multi-Threaded Static (/MT)"/>
            <enumeratedOptionValue
                  command="/MTd"
                  id="org.eclipse.cdt.windows.cl.option.runtime.mtd"
                  isDefault="false"
                  name="Multi-Threaded Static Debug (/MTd)"/>
            <enumeratedOptionValue
                  command="/MD"
                  id="org.eclipse.cdt.windows.cl.option.runtime.md"
                  isDefault="true"
                  name="Multi-Threaded DLL (/MD)"/>
            <enumeratedOptionValue
                  command="/MDd"
                  id="org.eclipse.cdt.windows.cl.option.runtime.mdd"
                  isDefault="false"
                  name="Multi-Threaded DLL Debug (/MDd)"/>
         </option>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.debug"
               name="Debugging"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.debug"
               id="org.eclipse.cdt.windows.cl.option.debugFormat"
               name="Debug Information Format"
               resourceFilter="all"
               valueType="enumerated">
            <enumeratedOptionValue
                  id="org.eclipse.cdt.windows.cl.option.debugFormat.none"
                  isDefault="true"
                  name="None"/>
            <enumeratedOptionValue
                  command="/Z7"
                  id="org.eclipse.cdt.windows.cl.option.debugFormat.c7"
                  isDefault="false"
                  name="C7 Compatible (/Z7)"/>
            <enumeratedOptionValue
                  command="/Zi"
                  id="org.eclipse.cdt.windows.cl.option.debugFormat.complete"
                  isDefault="false"
                  name="Complete (/Zi)"/>
            <enablement
                  attribute="value"
                  extensionAdjustment="false"
                  type="CONTAINER_ATTRIBUTE"
                  value="org.eclipse.cdt.windows.cl.option.debugFormat.none">
               <checkBuildProperty
                     property="org.eclipse.cdt.build.core.buildType"
                     value="org.eclipse.cdt.build.core.buildType.release">
               </checkBuildProperty>
            </enablement>
            <enablement
                  attribute="value"
                  extensionAdjustment="false"
                  type="CONTAINER_ATTRIBUTE"
                  value="org.eclipse.cdt.windows.cl.option.debugFormat.complete">
               <checkBuildProperty
                     property="org.eclipse.cdt.build.core.buildType"
                     value="org.eclipse.cdt.build.core.buildType.debug">
               </checkBuildProperty>
            </enablement>
         </option>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.preprocessor"
               name="Preprocessor"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.preprocessor"
               command="/D"
               id="org.eclipse.cdt.windows.cl.option.macros"
               name="Defines (/D)"
               resourceFilter="all"
               valueType="definedSymbols"/>
         <option
               browseType="directory"
               category="org.eclipse.cdt.windows.cl.optionCategory.preprocessor"
               command="/I"
               id="org.eclipse.cdt.windows.cl.option.includes"
               name="Include Path (/I)"
               resourceFilter="all"
               valueType="includePath"/>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.language"
               name="Language"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.language"
               command="/openmp"
               defaultValue="false"
               id="org.eclipse.cdt.windows.cl.option.openmp"
               name="Enable OpenMP (/openmp)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.language"
               commandFalse="/Za"
               defaultValue="true"
               id="org.eclipse.cdt.windows.cl.option.langExtensions"
               name="Enable Language Extensions (/Ze,/Za)"
               resourceFilter="all"
               valueType="boolean"/>
         <optionCategory
               id="org.eclipse.cdt.windows.cl.optionCategory.miscellaneous"
               name="Miscellaneous"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.miscellaneous"
               command="/nologo"
               defaultValue="true"
               id="org.eclipse.cdt.windows.cl.option.nologo"
               name="Suppress Banner (/nologo)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.cl.optionCategory.miscellaneous"
               id="org.eclipse.cdt.windows.cl.option.others"
               name="Other Options"
               resourceFilter="all"
               valueType="stringList"/>
         <inputType
               dependencyContentType="org.eclipse.cdt.core.cxxHeader"
               id="org.eclipse.cdt.windows.cl.inputType"
               sourceContentType="org.eclipse.cdt.core.cxxSource">
         </inputType>
         <outputType
               id="org.eclipse.cdt.windows.cl.outputType"
               outputs="obj">
         </outputType>
      </tool>
      <tool
            command="rc"
            errorParsers="org.eclipse.cdt.core.VCErrorParser"
            id="org.eclipse.cdt.windows.rc"
            name="%rc.name"
            natureFilter="both"
            outputFlag="/Fo"
            >
         <optionCategory
               id="org.eclipse.cdt.windows.rc.optionCategory.preprocessor"
               name="Preprocessor"/>
         <option
               browseType="directory"
               category="org.eclipse.cdt.windows.rc.optionCategory.preprocessor"
               command="/I"
               id="org.eclipse.cdt.windows.rc.option.includes"
               name="Include Path (/I)"
               resourceFilter="all"
               valueType="includePath"/>
         <option
               category="org.eclipse.cdt.windows.rc.optionCategory.preprocessor"
               command="/D"
               id="org.eclipse.cdt.windows.rc.option.defines"
               name="Defines (/D)"
               resourceFilter="all"
               valueType="definedSymbols"/>
         <optionCategory
               id="org.eclipse.cdt.windows.rc.optionCategory.misc"
               name="Miscellaneous"/>
         <option
               category="org.eclipse.cdt.windows.rc.optionCategory.misc"
               id="org.eclipse.cdt.windows.rc.option.others"
               name="Other Options"
               resourceFilter="all"
               valueType="stringList"/>
         <inputType
               id="org.eclipse.cdt.windows.rc.inputType"
               sources="rc">
         </inputType>
         <outputType
               id="org.eclipse.cdt.windows.rc.outputType"
               outputs="res">
         </outputType>
      </tool>
      <tool
            command="link"
            errorParsers="org.eclipse.cdt.core.VCErrorParser"
            id="org.eclipse.cdt.windows.link"
            name="%linker.name"
            natureFilter="both"
            outputFlag="/OUT:"
            >
         <supportedProperties>
            <property
                  id="org.eclipse.cdt.build.core.buildArtefactType">
               <value
                     id="org.eclipse.cdt.build.core.buildArtefactType.exe"></value>
               <value
                     id="org.eclipse.cdt.build.core.buildArtefactType.sharedLib">
               </value>
            </property>
         </supportedProperties>
         <envVarBuildPath
               pathType="buildpathLibrary"
               variableList="LIBS"/>
         <option
               command="/DLL"
               defaultValue="false"
               id="org.eclipse.cdt.windows.link.option.dll"
               name="Build a DLL"
               resourceFilter="all"
               valueType="boolean">
            <enablement
                  attribute="defaultValue"
                  extensionAdjustment="false"
                  type="CONTAINER_ATTRIBUTE"
                  value="true">
               <checkBuildProperty
                     property="org.eclipse.cdt.build.core.buildArtefactType"
                     value="org.eclipse.cdt.build.core.buildArtefactType.sharedLib">
               </checkBuildProperty>
            </enablement>
         </option>
         <optionCategory
               id="org.eclipse.cdt.windows.link.optionCategory.general"
               name="General"/>
         <option
               category="org.eclipse.cdt.windows.link.optionCategory.general"
               command="/debug"
               defaultValue="false"
               id="org.eclipse.cdt.windows.link.option.debug"
               name="Generate Debug Info (/debug)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.link.optionCategory.general"
               command="/nologo"
               defaultValue="true"
               id="org.eclipse.cdt.windows.link.option.nologo"
               name="Suppress Banner (/nologo)"
               resourceFilter="all"
               valueType="boolean"/>
         <option
               category="org.eclipse.cdt.windows.link.optionCategory.general"
               id="org.eclipse.cdt.windows.link.option.others"
               name="Other Options"
               resourceFilter="all"
               valueType="stringList"/>
         <optionCategory
               id="org.eclipse.cdt.windows.link.optionCategory.libraries"
               name="Libraries"/>
         <option
               browseType="directory"
               category="org.eclipse.cdt.windows.link.optionCategory.libraries"
               command="/libpath:"
               id="org.eclipse.cdt.windows.link.option.libpath"
               name="Additional Libpath (/libpath)"
               resourceFilter="all"
               valueType="stringList"/>
         <option
               browseType="file"
               category="org.eclipse.cdt.windows.link.optionCategory.libraries"
               id="org.eclipse.cdt.windows.link.option.libraries"
               name="Libraries"
               resourceFilter="all"
               valueType="libs"/>
         <inputType
               id="org.eclipse.cdt.windows.link.inputType"
               multipleOfType="true"
               sources="res,obj">
         </inputType>
         <outputType
               id="org.eclipse.cdt.windows.link.outputType"
               outputs="exe">
         </outputType>
         <outputType
               id="org.eclipse.cdt.windows.link.outputType.dll"
               outputs="dll">
         </outputType>
      </tool>
      <tool
            command="lib"
            id="org.eclipse.cdt.windows.lib"
            name="%lib.name"
            natureFilter="both"
            outputFlag="/OUT:"
            >
         <supportedProperties>
            <property
                  id="org.eclipse.cdt.build.core.buildArtefactType">
               <value
                     id="org.eclipse.cdt.build.core.buildArtefactType.staticLib">
               </value>
            </property>
         </supportedProperties>
         <optionCategory
               id="org.eclipse.cdt.windows.lib.optionCategory.general"
               name="General"/>
         <option
               category="org.eclipse.cdt.windows.lib.optionCategory.general"
               command="/nologo"
               defaultValue="true"
               id="org.eclipse.cdt.windows.lib.option.nologo"
               name="Suppress Banner (/nologo)"
               resourceFilter="all"
               valueType="boolean"/>
         <inputType
               id="org.eclipse.cdt.windows.lib.inputType"
               sources="obj,res">
         </inputType>
         <outputType
               id="org.eclipse.cdt.windows.lib.outputType"
               outputs="lib">
         </outputType>
      </tool>
      </toolChain>
      <configuration
            artifactExtension="exe"
            id="org.eclipse.cdt.windows.configuration">
         <enablement
               attribute="artifactExtension"
               extensionAdjustment="false"
               type="CONTAINER_ATTRIBUTE"
               value="dll">
            <checkBuildProperty
                  property="org.eclipse.cdt.build.core.buildArtefactType"
                  value="org.eclipse.cdt.build.core.buildArtefactType.sharedLib"></checkBuildProperty>
         </enablement>
         <enablement
               attribute="artifactExtension"
               extensionAdjustment="false"
               type="CONTAINER_ATTRIBUTE"
               value="lib">
            <checkBuildProperty
                  property="org.eclipse.cdt.build.core.buildArtefactType"
                  value="org.eclipse.cdt.build.core.buildArtefactType.staticLib"></checkBuildProperty>
         </enablement>
      </configuration>
      <projectType
            buildProperties="org.eclipse.cdt.build.core.buildArtefactType=org.eclipse.cdt.build.core.buildArtefactType.exe"
            id="org.eclipse.cdt.windows.projectType.exe"
            projectEnvironmentSupplier="org.eclipse.cdt.windows.build.WindowsEnvironmentVariableSupplier">
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.debug"
               id="org.eclipse.cdt.windows.exe.debug"
               name="%config.debug"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.exe.debug"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.exe.debug"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.exe.debug"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.exe.debug"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.link.exe.debug"
                     superClass="org.eclipse.cdt.windows.link">
               </tool>
            </toolChain>
         </configuration>
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.release"
               id="org.eclipse.cdt.windows.exe.release"
               name="%config.release"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.exe.release"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.exe.release"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.exe.release"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.exe.release"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.link.exe.release"
                     superClass="org.eclipse.cdt.windows.link">
               </tool>
            </toolChain>
         </configuration>
      </projectType>
      <projectType
            buildProperties="org.eclipse.cdt.build.core.buildArtefactType=org.eclipse.cdt.build.core.buildArtefactType.sharedLib"
            id="org.eclipse.cdt.windows.projectType.dll"
            projectEnvironmentSupplier="org.eclipse.cdt.windows.build.WindowsEnvironmentVariableSupplier">
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.debug"
               id="org.eclipse.cdt.windows.dll.debug"
               name="%config.debug"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.dll.debug"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.dll.debug"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.dll.debug"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.dll.debug"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.link.dll.debug"
                     superClass="org.eclipse.cdt.windows.link">
               </tool>
            </toolChain>
         </configuration>
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.release"
               id="org.eclipse.cdt.windows.dll.release"
               name="%config.release"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.dll.release"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.dll.release"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.dll.release"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.dll.release"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.link.dll.release"
                     superClass="org.eclipse.cdt.windows.link">
               </tool>
            </toolChain>
         </configuration>
      </projectType>
      <projectType
            buildProperties="org.eclipse.cdt.build.core.buildArtefactType=org.eclipse.cdt.build.core.buildArtefactType.staticLib"
            id="org.eclipse.cdt.windows.projectType.lib"
            projectEnvironmentSupplier="org.eclipse.cdt.windows.build.WindowsEnvironmentVariableSupplier">
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.debug"
               id="org.eclipse.cdt.windows.lib.debug"
               name="%config.debug"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.lib.debug"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.lib.debug"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.lib.debug"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.lib.debug"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.lib.lib.debug"
                     superClass="org.eclipse.cdt.windows.lib">
               </tool>
            </toolChain>
         </configuration>
         <configuration
               buildProperties="org.eclipse.cdt.build.core.buildType=org.eclipse.cdt.build.core.buildType.release"
               id="org.eclipse.cdt.windows.lib.release"
               name="%config.release"
               parent="org.eclipse.cdt.windows.configuration">
            <toolChain
                  id="org.eclipse.cdt.windows.toolchain.lib.release"
                  superClass="org.eclipse.cdt.windows.toolchain.base">
               <targetPlatform
                     id="org.eclipse.cdt.windows.targetPlatform.lib.release"
                     superClass="org.eclipse.cdt.windows.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.windows.cl.lib.release"
                     superClass="org.eclipse.cdt.windows.cl">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.rc.lib.release"
                     superClass="org.eclipse.cdt.windows.rc">
               </tool>
               <tool
                     id="org.eclipse.cdt.windows.lib.lib.release"
                     superClass="org.eclipse.cdt.windows.lib">
               </tool>
            </toolChain>
         </configuration>
      </projectType>
   </extension>

</plugin>
