<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.cdt.managedbuilder.core.buildDefinitions">
      <managedBuildRevision
            fileVersion="4.0.0">
      </managedBuildRevision>
      <toolChain
            configurationEnvironmentSupplier="org.eclipse.cdt.csharp.build.CSharpEnvironmentSupplier"
            id="org.eclipse.cdt.csharp.toolChain.base"
            isAbstract="false"
            name="Microsoft Visual C#"
            targetTool="org.eclipse.cdt.csharp.csc">
         <targetPlatform
               archList="all"
               binaryParser="org.eclipse.cdt.core.PE"
               id="org.eclipse.cdt.csharp.targetPlatform"
               isAbstract="false"
               osList="win32">
         </targetPlatform>
         <builder
               id="org.eclipse.cdt.csharp.builder"
               isAbstract="false"
               isVariableCaseSensitive="false"
               superClass="org.eclipse.cdt.build.core.internal.builder">
         </builder>
         <tool
               command="csc"
               id="org.eclipse.cdt.csharp.csc"
               name="C# Compiler"
               outputFlag="/out:">
            <supportedProperties>
               <property
                     id="org.eclipse.cdt.build.core.buildArtefactType">
                  <value
                        id="org.eclipse.cdt.build.core.buildArtefactType.exe"></value>
               </property>
            </supportedProperties>
            <inputType
                  id="org.eclipse.cdt.csharp.csc.inputType"
                  multipleOfType="true"
                  primaryInput="true"
                  sources="cs">
            </inputType>
            <outputType
                  id="org.eclipse.cdt.csharp.csc.outputType"
                  outputs="exe">
            </outputType>
         </tool>
      </toolChain>
      <configuration
            artifactExtension="exe"
            id="org.eclipse.cdt.csharp.configuration.base">
      </configuration>
      <projectType
            buildProperties="org.eclipse.cdt.build.core.buildArtefactType=org.eclipse.cdt.build.core.buildArtefactType.exe"
            id="org.eclipse.cdt.csharp.projectType.exe"
            >
         <configuration
               id="org.eclipse.cdt.csharp.configuration.exe.debug"
               name="Debug"
               parent="org.eclipse.cdt.csharp.configuration.base">
            <toolChain
                  id="org.eclipse.cdt.csharp.toolChain.exe.debug"
                  superClass="org.eclipse.cdt.csharp.toolChain.base">
               <targetPlatform
                     id="org.eclipse.cdt.csharp.targetPlatform.exe.debug"
                     superClass="org.eclipse.cdt.csharp.targetPlatform">
               </targetPlatform>
               <tool
                     id="org.eclipse.cdt.csharp.csc.exe.debug"
                     superClass="org.eclipse.cdt.csharp.csc">
               </tool>
            </toolChain>
         </configuration>
      </projectType>
   </extension>

</plugin>
